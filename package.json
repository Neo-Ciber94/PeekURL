{
  "name": "peek-url",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:debug": "npx cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "next:dev-local": "ts-node --project ./tsconfig.server.json ./local/server.ts",
    "next:start-local": "cross-env NODE_ENV=production ts-node --project ./tsconfig.server.json ./local/server.ts",
    "typecheck": "tsc --noEmit",
    "scripts:tld": "npx cross-env NODE_OPTIONS=--experimental-fetch npx ts-node --project ./tsconfig.node.json ./scripts/load-tld.ts",
    "deploy": "npx prisma generate && npx prisma migrate deploy && npm run build",
    "test": "vitest --run",
    "test:watch": "vitest",
    "clean": "rimraf .next",
    "clean:node-modules": "rimraf node_modules"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@hookform/resolvers": "^2.9.6",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.90",
    "@mui/material": "^5.9.0",
    "@prisma/client": "^4.0.0",
    "@reduxjs/toolkit": "^1.8.3",
    "@trpc/client": "^9.26.0",
    "@trpc/next": "^9.26.0",
    "@trpc/react": "^9.26.0",
    "@trpc/server": "^9.26.0",
    "@types/fs-extra": "^9.0.13",
    "firebase": "^9.9.0",
    "firebase-admin": "^11.0.0",
    "nanoid": "^4.0.0",
    "next": "^12.2.3",
    "node-fetch": "^2.6.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.33.1",
    "react-query": "^3.39.1",
    "react-redux": "^8.0.2",
    "react-social-login-buttons": "^3.6.0",
    "react-spring": "^9.5.2",
    "redis": "^4.2.0",
    "superjson": "^1.9.1",
    "vercel": "^27.3.3",
    "winston": "^3.8.1",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/geoip-lite": "^1.4.1",
    "@types/node": "18.0.4",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "cross-env": "^7.0.3",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.2",
    "eslint-config-prettier": "^8.5.0",
    "express": "^4.18.1",
    "jsdom": "^20.0.0",
    "prisma": "^4.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "4.7.4",
    "vitest": "^0.18.1"
  }
}
